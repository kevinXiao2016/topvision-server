function tabFn(index){
	switch (index){
		case 0:
			$(".tabBody").css("display","block");
			break;
		default:
			$(".tabBody").css("display","none");
			$(".tabBody").eq(index-1).stop().fadeIn();
			break;
};//end switch;	
