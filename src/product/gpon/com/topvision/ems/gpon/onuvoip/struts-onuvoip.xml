<struts>
	<package name="gponOnuVoip" namespace="/gpon/onuvoip" extends="default">
	    <action name="showSIPPstnUser" class="onuVoipAction" method="showSIPPstnUser">
            <result>/gpon/onuvoip/sipPstnUser.jsp</result>
        </action>
        <action name="loadSipPstnUser" class="onuVoipAction" method="loadSipPstnUser" />
        <action name="modifySipPstnUser" class="onuVoipAction" method="modifySipPstnUser" />
