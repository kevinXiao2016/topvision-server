<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.topvision.ems.cmc.flap.domain.CmFlapHis">	<select id="getCmIdByMap" parameterType="map" resultType="long">        select cmId from cmccmrelation where        cmcId=#{cmcId} and maclong=#{maclong}    </select>		<!-- ################################## select end ######################################### -->	<insert id="insertCmFlapHis" parameterType="cmFlapHis">		insert into cmflaphis		(cmId,cmcId,cmMac,collectTime,lastFlapTime,insFailNum,increaseInsNum,hitNum,missNum,increaseHitPercent,crcErrorNum,powerAdjLowerNum,powerAdjHigherNum,increasePowerAdjNum)		values		(#{cmId},#{cmcId},#{cmMac},#{collectTime},#{lastFlapTime},#{insFailNum},#{increaseInsNum},#{hitNum},#{missNum},#{increaseHitPercent},#{crcErrorNum},#{powerAdjLowerNum},#{powerAdjHigherNum},#{increasePowerAdjNum})	</insert>	</mapper>