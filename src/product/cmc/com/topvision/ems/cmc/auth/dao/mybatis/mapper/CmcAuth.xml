<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.topvision.ems.cmc.auth.domain.CmcAuth"><!--     <typeAlias alias="ccmtsAuthManagement" type="com.topvision.ems.cmc.facade.domain.CcmtsAuthManagement" /> -->    <!-- ################################## select start ####################################### -->    <select id="selectCmcAuthInfo" parameterType="Long" resultType="ccmtsAuthManagement">        SELECT             cmcAuthCurrentInfo AS topCcmtsAuthCurrentInfo        FROM             cmcAttribute        WHERE             cmcId=#{cmcId}    </select>    <select id="selectCmcIndexByCmcId" parameterType="Long" resultType="Long">        SELECT             cmcIndex AS ifIndex        FROM             cmcentityrelation        WHERE             cmcId=#{cmcId}    </select>    <!-- ################################## select end   ####################################### -->		<!-- ################################## insert start ####################################### -->	<!-- ################################## insert end ######################################### -->		<!-- ################################## update start ####################################### -->	<update id="updateCmcAuthInfo" parameterType="ccmtsAuthManagement">		UPDATE		cmcAttribute		SET		cmcAuthCurrentInfo=#{topCcmtsAuthCurrentInfo}		WHERE		cmcId=#{cmcId}	</update>	<!-- ################################## update end ######################################### -->		<!-- ################################## delete start ####################################### -->	<!-- ################################## delete end ######################################### --></mapper>