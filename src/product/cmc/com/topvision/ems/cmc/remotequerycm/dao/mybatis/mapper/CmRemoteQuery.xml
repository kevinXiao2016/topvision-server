<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.topvision.ems.cmc.remotequerycm.domain.CmRemoteQuery">    <select id="getUpChanInfo" parameterType="map"         resultType="com.topvision.ems.cmc.upchannel.facade.domain.CmcUpChannelBaseInfo">        SELECT         	*         FROM        	cmcupchannelbaseinfo         WHERE         	cmcId = #{cmcId}        AND         	channelIndex = #{channelIndex}    </select>        <select id="getUpChanInfoByChanId" parameterType="map"         resultType="com.topvision.ems.cmc.upchannel.facade.domain.CmcUpChannelBaseInfo">        SELECT         	*         FROM        	cmcupchannelbaseinfo         WHERE         	cmcId = #{cmcId}        AND         	channelId = #{channelId}    </select>         <select id="getDownChanInfo" parameterType="map"         resultType="com.topvision.ems.cmc.facade.domain.CmcDownChannelBaseInfo">        SELECT         	*         FROM        	cmcdownchannelbaseinfo         WHERE         	cmcId = #{cmcId}        AND         	channelIndex = #{channelIndex}    </select>         <select id="getDownChanInfoByChanId" parameterType="map"         resultType="com.topvision.ems.cmc.facade.domain.CmcDownChannelBaseInfo">        SELECT         	*         FROM        	cmcdownchannelbaseinfo         WHERE         	cmcId = #{cmcId}        AND         	docsIfDownChannelId = #{channelId}    </select>          <select id="getOltIpByOnuId" parameterType="long" resultType="string">         SELECT          	entity.ip          FROM          	oltonuattribute,entity          WHERE          	oltonuattribute.entityId = entity.entityId          AND          	oltonuattribute.onuId = #{cmcId}     </select></mapper>