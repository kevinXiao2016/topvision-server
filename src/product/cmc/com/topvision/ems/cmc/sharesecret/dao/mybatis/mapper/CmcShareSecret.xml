<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.topvision.ems.cmc.sharesecret.domain.CmcShareSecret"><!--     <typeAlias alias="cmcShareSecretConfig" type="com.topvision.ems.cmc.sharesecret.facade.domain.CmcShareSecretConfig" />     -->    <!-- ##################### select area start ####################### -->    <select id="getCmcShareSecret" parameterType="Long" resultType="cmcShareSecretConfig">    SELECT cmcId, sharedSecretEnabled, sharedSecretEncrypted, sharedSecretAuthStr,         sharedSecretCipherStr    FROM cmcsharesecretconfig     WHERE cmcId=#{cmcId}    </select>    <!-- ##################### select area end   ####################### -->        <!-- ##################### update area start ####################### -->    <update id="updateCmcShareSecret" parameterType="cmcShareSecretConfig">    UPDATE cmcsharesecretconfig    SET        sharedSecretEnabled=#{sharedSecretEnabled},        sharedSecretEncrypted=#{sharedSecretEncrypted},        sharedSecretAuthStr=#{sharedSecretAuthStr},        sharedSecretCipherStr=#{sharedSecretCipherStr}    WHERE cmcId=#{cmcId}    </update>    <!-- ##################### update area end   ####################### -->        <!-- ##################### insert area start ####################### -->    <insert id="insertCmcShareSecretConfig" parameterType="cmcShareSecretConfig">        insert into cmcsharesecretconfig        (cmcId,        sharedSecretEnabled, sharedSecretEncrypted, sharedSecretAuthStr, sharedSecretCipherStr)        values        (#{cmcId},        #{sharedSecretEnabled}, #{sharedSecretEncrypted}, #{sharedSecretAuthStr}, #{sharedSecretCipherStr})    </insert>    <!-- ##################### insert area end   ####################### --></mapper>