        <action name="deleteLoadBalanceGroup" class="cmcLoadBalanceAction" method="deleteLoadBalanceGroup" />
        <action name="modifyLoadBalanceChannel" class="cmcLoadBalanceAction" method="modifyLoadBalanceChannel" />
        <action name="modifyLoadBalanceMacRanges" class="cmcLoadBalanceAction" method="modifyLoadBalanceMacRanges" />
