            <result>/cmc/modifyLoadBalPolicy.jsp</result>
        </action>
        <action name="enterLoadBalPolicyAddPage" class="cmcLoadBalanceAction" method="enterLoadBalPolicyAddPage">
            <result>/cmc/addLoadBalPolicy.jsp</result>
        </action>
        <action name="modifyLoadBalPolicy" class="cmcLoadBalanceAction" method="modifyLoadBalPolicy" />
        <action name="enterLoadBalPolicyListPage" class="cmcLoadBalanceAction" method="enterLoadBalPolicyListPage">
            <result>/cmc/loadBalPolicyList.jsp</result>
        </action>
        <action name="getLoadBalPolicyList" class="cmcLoadBalanceAction" method="getLoadBalPolicyList" />
        <action name="addLoadBalPolicy" class="cmcLoadBalanceAction" method="addLoadBalPolicy" />
        <action name="deleteLoadBalPolicy" class="cmcLoadBalanceAction" method="deleteLoadBalPolicy" />
        <action name="createOrModifyLoadbalance" class="cmcLoadBalanceAction" method="createOrModifyLoadbalance" />
        <action name="modifyCcmtsPolicy" class="cmcLoadBalanceAction" method="modifyCcmtsPolicy" />
