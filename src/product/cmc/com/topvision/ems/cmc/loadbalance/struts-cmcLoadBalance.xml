<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="cmcLoadBalance" namespace="/cmc/loadbalance" extends="default">
		<action name="getLoadBalanceGroups" class="cmcLoadBalanceAction"
			method="getLoadBalanceGroups">
		</action>
		<action name="showLoadbalance" class="cmcLoadBalanceAction" method="showLoadbalance">
			<result>/cmc/loadbalance.jsp</result>
		</action>
		<action name="showLoadBalanceAddition" class="cmcLoadBalanceAction" method="showLoadBalanceAddition">
			<result>/cmc/loadBalanceAddition.jsp</result>
		</action>
		<action name="showExcMacRangeAddition" class="cmcLoadBalanceAction" method="showExcMacRangeAddition">
			<result>/cmc/showExcMacRangeAddition.jsp</result>
