<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.topvision.ems.cmc.acl.domain.CmcAcl"><!-- 	<typeAlias alias="cmcAclInfo" type="com.topvision.ems.cmc.facade.domain.CmcAclInfo"/>	<typeAlias alias="cmcAclDefAct" type="com.topvision.ems.cmc.domain.CmcAclDefAct"/> -->	<!-- ################################## select start ####################################### -->	<select id="getAclDefAct" parameterType="map" resultType="cmcAclDefAct">		select		entityId,uplinkIngress  ,uplinkEgress  ,cableEgress  ,cabelIngress 		from cmcacldefact		where entityId = #{entityId}	</select>	<select id="getOnePositionAclList" parameterType="map"		resultType="cmcAclInfo">		select		entityId,topCcmtsAclListIndex,topCcmtsAclDesc,topCcmtsAclPrio,topMatchlSrcMac,topMatchSrcMacMask,topMatchDstMac,topMatchDstMacMask,		topMatchVlanId,topMatchVlanCos,topMatchEtherType,topMatchDscp,topMatchIpProtocol,topMatchSrcIp,topMatchSrcIpMask,topMatchDstIp,topMatchDstIpMask,		topMatchSrcPort,topMatchDstPort,topActionMask,topActionAddVlanId,topActionAddVlanCos,topActionNewVlanCos,installPosition 		from cmcaclconfig		where entityId = #{entityId} and installPosition like #{installPosition}	</select>	<select id="getAllAclList" parameterType="map"		resultType="cmcAclInfo">		select		entityId,topCcmtsAclListIndex,topCcmtsAclDesc,topCcmtsAclPrio,topMatchlSrcMac,topMatchSrcMacMask,topMatchDstMac,topMatchDstMacMask,		topMatchVlanId,topMatchVlanCos,topMatchEtherType,topMatchDscp,topMatchIpProtocol,topMatchSrcIp,topMatchSrcIpMask,topMatchDstIp,topMatchDstIpMask,		topMatchSrcPort,topMatchDstPort,topActionMask,topActionAddVlanId,topActionAddVlanCos,topActionNewVlanCos,installPosition 		from cmcaclconfig		where entityId = #{entityId}	</select>	<select id="getOneAcl" parameterType="map"		resultType="cmcAclInfo">		select		entityId,topCcmtsAclListIndex,topCcmtsAclDesc,topCcmtsAclPrio,topMatchlSrcMac,topMatchSrcMacMask,topMatchDstMac,topMatchDstMacMask,		topMatchVlanId,topMatchVlanCos,topMatchEtherType,topMatchDscp,topMatchIpProtocol,topMatchSrcIp,topMatchSrcIpMask,topMatchDstIp,topMatchDstIpMask,		topMatchSrcPort,topMatchDstPort,topActionMask,topActionAddVlanId,topActionAddVlanCos,topActionNewVlanCos,topActionNewVlanTpid,		topActionNewVlanCfi,topActionNewVlanId,topActionAddVlanTpid,topActionAddVlanCfi,topActionNewIpTos,topActionNewIpDscp,installPosition 		from cmcaclconfig		where entityId = #{entityId} and topCcmtsAclListIndex = #{topCcmtsAclListIndex}	</select>			<!-- ################################## select end ######################################### -->		<!-- ################################## insert start ####################################### -->	<insert id="insertAclDefAct" parameterType="map">		insert into cmcacldefact		(entityId,uplinkIngress  ,uplinkEgress  ,cableEgress  ,cabelIngress)		values		(#{entityId},#{uplinkIngress},#{uplinkEgress},#{cableEgress},#{cabelIngress})	</insert>	<insert id="addAclInfo" parameterType="cmcAclInfo">		insert into cmcaclconfig		(entityId,topCcmtsAclListIndex,topCcmtsAclDesc,topCcmtsAclPrio,topMatchlSrcMac,topMatchSrcMacMask,topMatchDstMac,topMatchDstMacMask,		topMatchVlanId,topMatchVlanCos,topMatchEtherType,topMatchDscp,topMatchIpProtocol,topMatchSrcIp,topMatchSrcIpMask,topMatchDstIp,topMatchDstIpMask,		topMatchSrcPort,topMatchDstPort,topActionMask,topActionAddVlanId,topActionAddVlanCos,topActionNewVlanCos,topActionNewVlanTpid,		topActionNewVlanCfi,topActionNewVlanId,topActionAddVlanTpid,topActionAddVlanCfi,topActionNewIpTos,topActionNewIpDscp,installPosition)		values		(#{entityId},#{topCcmtsAclListIndex},#{topCcmtsAclDesc},#{topCcmtsAclPrio},#{topMatchlSrcMac},#{topMatchSrcMacMask},#{topMatchDstMac},		#{topMatchDstMacMask},#{topMatchVlanId},#{topMatchVlanCos},#{topMatchEtherType},#{topMatchDscp},#{topMatchIpProtocol},#{topMatchSrcIp},#{topMatchSrcIpMask},		#{topMatchDstIp},#{topMatchDstIpMask},#{topMatchSrcPort},#{topMatchDstPort},#{topActionMask},#{topActionAddVlanId},#{topActionAddVlanCos},		#{topActionNewVlanCos},#{topActionNewVlanTpid},#{topActionNewVlanCfi},#{topActionNewVlanId},#{topActionAddVlanTpid},#{topActionAddVlanCfi},		#{topActionNewIpTos},#{topActionNewIpDscp},#{installPosition})	</insert>	<!-- ################################## insert end ######################################### -->		<!-- ################################## update start ####################################### -->	<!-- ################################## update end ######################################### -->		<!-- ################################## delete start ####################################### -->	<delete id="deleteAllAclDefAct" parameterType="map">		DELETE FROM		cmcacldefact WHERE entityId = #{entityId}	</delete>	<delete id="deleteAllAcl" parameterType="map">		DELETE FROM		cmcaclconfig WHERE entityId = #{entityId}	</delete>	<delete id="deleteOneAcl" parameterType="map">		DELETE FROM		cmcaclconfig WHERE entityId = #{entityId} and topCcmtsAclListIndex = #{topCcmtsAclListIndex}	</delete>	<!-- ################################## delete end ######################################### --></mapper>