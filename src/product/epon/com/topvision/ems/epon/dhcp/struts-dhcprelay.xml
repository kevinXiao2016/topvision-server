<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="dhcprelay" namespace="/epon/dhcprelay" extends="default">
		<action name="showDhcpRelayConfigList" class="oltDhcpRelayAction" method="showDhcpRelayConfigList">
			<result>/epon/dhcprelay/oltDhcpRelayConfigList.jsp</result>
		</action>
		<action name="showModifyDhcpRelayConfig" class="oltDhcpRelayAction" method="showModifyDhcpRelayConfig">
			<result>/epon/dhcprelay/createDhcpRelayConfig.jsp</result>
		</action>
		<action name="getDhcpRelayConfigList" class="oltDhcpRelayAction" method="getDhcpRelayConfigList" />
		<action name="deleteDhcpRelayConfig" class="oltDhcpRelayAction" method="deleteDhcpRelayConfig" />
		<action name="modifyDhcpRelaySwitch" class="oltDhcpRelayAction" method="modifyDhcpRelaySwitch" />
		<action name="modifyDhcpRelayBundleConfig" class="oltDhcpRelayAction" method="modifyDhcpRelayBundleConfig" />
		<action name="refreshDhcpRelayConfigFromDevice" class="oltDhcpRelayAction" method="refreshDhcpRelayConfigFromDevice" />
	</package>
</struts>