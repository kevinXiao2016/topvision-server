<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="iproute" namespace="/epon/iproute" extends="default">
		<action name="showIpRouteConfig" class="ipRouteConfigAction" method="showIpRouteConfig">
			<result>/epon/iproute/ipRouteConfig.jsp</result>
		</action>
		<action name="loadIpRouteList" class="ipRouteConfigAction" method="loadIpRouteList" />
		<action name="addIpRouteConfig" class="ipRouteConfigAction" method="addIpRouteConfig" />
		<action name="modifyIpRouteConfig" class="ipRouteConfigAction" method="modifyIpRouteConfig" />
		<action name="deleteIpRouteConfig" class="ipRouteConfigAction" method="deleteIpRouteConfig" />
		<action name="refreshIpRouteConfig" class="ipRouteConfigAction" method="refreshIpRouteConfig" />
	</package>
</struts>
