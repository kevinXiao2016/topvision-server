<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="loopback" namespace="/epon/loopback" extends="default">
		<action name="showLoopBackConfig" class="loopBackConfigAction" method="showLoopBackConfig">
			<result>/epon/loopback/loopBackConfig.jsp</result>
		</action>
		<action name="loadLBInterfaceList" class="loopBackConfigAction" method="loadLBInterfaceList" />
		<action name="addLBInterface" class="loopBackConfigAction" method="addLBInterface" />
		<action name="deleteLBInterface" class="loopBackConfigAction" method="deleteLBInterface" />
		<action name="modifyLoopBack" class="loopBackConfigAction" method="modifyLoopBack" />
		<action name="refreshLoopBackInterface" class="loopBackConfigAction" method="refreshLoopBackInterface" />
		
		<action name="showLBSubIpConfig" class="loopBackConfigAction" method="showLBSubIpConfig" >
			<result>/epon/loopback/loopBackSubIpConfig.jsp</result>
		</action>
		<action name="loadLBSubIpList" class="loopBackConfigAction" method="loadLBSubIpList" />
		<action name="addLBSubIp" class="loopBackConfigAction" method="addLBSubIp" />
		<action name="deleteLBSubIp" class="loopBackConfigAction" method="deleteLBSubIp" />
		<action name="modifyLBSubIp" class="loopBackConfigAction" method="modifyLBSubIp" />
		<action name="refreshLBSubIp" class="loopBackConfigAction" method="refreshLBSubIp" />
	</package>
</struts>
