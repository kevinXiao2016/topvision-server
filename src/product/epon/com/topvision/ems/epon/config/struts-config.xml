		<action name="modifySnmpV2CCfg" class="oltConfigInfoAction" method="modifySnmpV2CCfg" />
		<action name="downFile" class="oltConfigFileAction" method="downFile" />
		<action name="refreshOltConfigInfo" class="oltConfigInfoAction" method="refreshOltConfigInfo" />
	</package>
</struts>
