<struts>
	<package name="config" namespace="/config" extends="default">
		<action name="showOltConfigJsp" class="oltConfigInfoAction" method="showOltConfigJsp">
            <result>/epon/oltConfigInfo.jsp</result>
        </action>
        <action name="loadAvailableVlanList" class="oltConfigInfoAction" method="loadAvailableVlanList" />
		<action name="saveBasicConfig" class="oltConfigInfoAction" method="saveBasicConfig" />
		<action name="saveInbandParamAndIP" class="oltConfigInfoAction" method="saveInbandParamAndIP" />
